{
  "openapi": "3.0.1",
  "info": {
    "title": "AriaVerse API",
    "description": "基于SpringBoot + Spring AI的智能助手AriaVerse项目",
    "contact": {
      "name": "Jinyu",
      "email": "jinyu067@gmail.com"
    },
    "version": "v0.0.1"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Generated server url"
    }
  ],
  "tags": [
    {
      "name": "AI聊天接口",
      "description": "提供AI聊天功能的接口"
    },
    {
      "name": "会话管理接口",
      "description": "提供会话管理功能的接口"
    },
    {
      "name": "登录注册接口",
      "description": "提供登录和注册功能的接口"
    }
  ],
  "paths": {
    "/api/aria/session": {
      "post": {
        "tags": [
          "会话管理接口"
        ],
        "summary": "创建新的聊天会话",
        "operationId": "createChatSession",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "string"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultString"
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/auth/register": {
      "post": {
        "tags": [
          "登录注册接口"
        ],
        "summary": "用户注册",
        "description": "注册新用户",
        "operationId": "register",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegisterDTO"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Result"
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/auth/login": {
      "post": {
        "tags": [
          "登录注册接口"
        ],
        "summary": "用户登录",
        "operationId": "login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginDTO"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultLoginVO"
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/ai/chat": {
      "post": {
        "tags": [
          "AI聊天接口"
        ],
        "summary": "聊天接口",
        "description": "与AI进行对话，返回AI生成的回复",
        "operationId": "chat",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/ChatDTO"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/event-stream;charset=UTF-8": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/session/list": {
      "get": {
        "tags": [
          "会话管理接口"
        ],
        "summary": "获取历史会话列表，游标分页",
        "operationId": "listChatSessions",
        "parameters": [
          {
            "name": "lastConversationId",
            "in": "query",
            "description": "最后一次查询到的会话Id",
            "required": false,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "单页大小",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultListChatSessionVO"
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/session/history/{conversationId}": {
      "get": {
        "tags": [
          "会话聊天消息管理接口"
        ],
        "summary": "查询指定会话的聊天记录",
        "operationId": "getChatHistory",
        "parameters": [
          {
            "name": "conversationId",
            "in": "path",
            "description": "会话ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "lastMessageTimeStamp",
            "in": "query",
            "description": "最后查询到消息的时间戳",
            "required": false,
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "pageSize",
            "in": "query",
            "description": "单页查询大小",
            "required": false,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultListChatMessageVO"
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/session/delete/{conversationId}": {
      "delete": {
        "tags": [
          "会话管理接口"
        ],
        "summary": "删除聊天会话",
        "operationId": "deleteChatSession",
        "parameters": [
          {
            "name": "conversationId",
            "in": "path",
            "description": "会话ID",
            "required": true,
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "clearChatMemory",
            "in": "query",
            "description": "是否清理聊天记录",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": true
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultBoolean"
                }
              }
            }
          }
        }
      }
    },
    "/api/aria/session/batch-delete": {
      "delete": {
        "tags": [
          "会话管理接口"
        ],
        "summary": "批量删除聊天会话",
        "operationId": "batchDeleteChatSessions",
        "parameters": [
          {
            "name": "clearChatMemory",
            "in": "query",
            "description": "是否清理聊天记录",
            "required": false,
            "schema": {
              "type": "boolean",
              "default": true
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "array",
                "description": "会话ID列表",
                "items": {
                  "type": "string"
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ResultInteger"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ResultString": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "string"
          }
        }
      },
      "RegisterDTO": {
        "required": [
          "email",
          "password",
          "username"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "用户邮箱",
            "example": "user@example.com",
            "refType": null
          },
          "username": {
            "type": "string",
            "description": "用户名",
            "example": "username",
            "refType": null
          },
          "password": {
            "type": "string",
            "description": "密码",
            "example": "password123",
            "refType": null
          }
        },
        "description": "用户注册数据传输对象"
      },
      "Result": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "object"
          }
        }
      },
      "LoginDTO": {
        "required": [
          "email",
          "password"
        ],
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "description": "用户邮箱",
            "example": "user@example.com",
            "refType": null
          },
          "password": {
            "type": "string",
            "description": "密码",
            "example": "password123",
            "refType": null
          }
        },
        "description": "用户登录数据传输对象"
      },
      "LoginVO": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int64",
            "refType": null
          },
          "email": {
            "type": "string",
            "refType": null
          },
          "username": {
            "type": "string",
            "refType": null
          }
        }
      },
      "ResultLoginVO": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "$ref": "#/components/schemas/LoginVO"
          }
        }
      },
      "ChatDTO": {
        "type": "object",
        "properties": {
          "prompt": {
            "type": "string",
            "description": "用户输入的提示词",
            "example": "你好，今天的天气怎么样？"
          },
          "conversationId": {
            "type": "string",
            "description": "会话Id",
            "example": "1234567890"
          }
        },
        "description": "聊天数据传输对象"
      },
      "ChatSessionVO": {
        "type": "object",
        "properties": {
          "conversationId": {
            "type": "string"
          },
          "userId": {
            "type": "integer",
            "format": "int64"
          },
          "title": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "updatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "lastMessage": {
            "type": "string"
          }
        }
      },
      "ResultListChatSessionVO": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChatSessionVO"
            }
          }
        }
      },
      "ChatMessageVO": {
        "type": "object",
        "properties": {
          "userId": {
            "type": "integer",
            "format": "int64"
          },
          "conversationId": {
            "type": "string"
          },
          "textContent": {
            "type": "string"
          },
          "messageType": {
            "type": "string"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "ResultListChatMessageVO": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ChatMessageVO"
            }
          }
        }
      },
      "ResultBoolean": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "boolean"
          }
        }
      },
      "ResultInteger": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer",
            "format": "int32"
          },
          "message": {
            "type": "string"
          },
          "data": {
            "type": "integer",
            "format": "int32"
          }
        }
      }
    }
  },
  "x-openapi": {
    "x-setting": {
      "customCode": 200,
      "language": "zh-CN",
      "enableSwaggerModels": true,
      "swaggerModelName": "Swagger Models",
      "enableReloadCacheParameter": false,
      "enableAfterScript": true,
      "enableDocumentManage": true,
      "enableVersion": false,
      "enableRequestCache": true,
      "enableFilterMultipartApis": false,
      "enableFilterMultipartApiMethodType": "POST",
      "enableHost": false,
      "enableHostText": "",
      "enableDynamicParameter": false,
      "enableDebug": true,
      "enableFooter": true,
      "enableFooterCustom": false,
      "enableSearch": true,
      "enableOpenApi": true,
      "enableHomeCustom": false,
      "enableGroup": true,
      "enableResponseCode": true
    },
    "x-markdownFiles": []
  }
}